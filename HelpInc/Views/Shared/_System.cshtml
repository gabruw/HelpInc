<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HelpInc</title>

    @* Framework's CSS *@
    <link rel="stylesheet" type="text/css" href="~/lib/semantic/semantic.css">

    @* Link's JS *@
    <script type="text/javascript" src="~/js/Generic/Side-menu.js"></script>

    @* Framework's JS *@
    <script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
    <script type="text/javascript" src="~/lib/semantic/semantic.js"></script>
</head>
<body>
    <div class="ui sidebar inverted vertical menu">
        <a class="item">1</a>
        <a class="item">2</a>
        <a class="item">3</a>
    </div>

    <div class="pusher">
        <button id="menu-lateral">Ativar</button>
        <div class="container">
            <partial name="_CookieConsentPartial" />
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

    @RenderSection("Scripts", required: false)
</body>
</html>
